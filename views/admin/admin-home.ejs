
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Dashboard </h2>
                    <p>Whole data about your business here</p>
                </div>
                <div>
                    <!-- <a href="/admin/sales-report-pdf" class="btn btn-primary " data-bs-toggle="tooltip" data-bs-placement="top" title="Download sales repord in pdf format"><img src="/admin/assets/imgs/icons/pdf-file.png" alt="Flat Icon" width="25" height="20">
                        Create report</a> -->

                    <a href="/admin/sales-report-page" class="btn btn-primary "><i class="text-muted material-icons md-post_add"></i>Create report</a>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3">
                    <div class="card card-body mb-4">
                        <article class="icontext">
                            <span class="icon icon-sm rounded-circle bg-primary-light"><i class="text-primary material-icons md-monetization_on"></i></span>
                            <div class="text">
                                <h6 class="mb-1 card-title">Revenue</h6>
                                <span><%= dashboardDetails.totalRevenue %></span>
                                <span class="text-sm">
                                    Shipping fees are not included
                                </span>
                            </div>
                        </article>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="card card-body mb-4">
                        <article class="icontext">
                            <span class="icon icon-sm rounded-circle bg-success-light"><i class="text-success material-icons md-local_shipping"></i></span>
                            <div class="text">
                                <h6 class="mb-1 card-title">Orders</h6> <span><%= dashboardDetails.totalOrders %></span>
                                <span class="text-sm">
                                    Excluding orders in transit
                                </span>
                            </div>
                        </article>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="card card-body mb-4">
                        <article class="icontext">
                            <span class="icon icon-sm rounded-circle bg-warning-light"><i class="text-warning material-icons md-qr_code"></i></span>
                            <div class="text">
                                <h6 class="mb-1 card-title">Products</h6> <span><%= dashboardDetails.totalProducts %></span>
                                <span class="text-sm">
                                    In <%= dashboardDetails.numberOfCategories %> Categories
                                </span>
                            </div>
                        </article>
                    </div>
                </div>
                <div class="col-lg-3">
                    <div class="card card-body mb-4">
                        <article class="icontext">
                            <span class="icon icon-sm rounded-circle bg-info-light"><i class="text-info material-icons md-shopping_basket"></i></span>
                            <div class="text">
                                <h6 class="mb-1 card-title">Monthly Earning</h6> <span><%= dashboardDetails.monthlyRevenue %></span>
                                <span class="text-sm">
                                    Based in your local time.
                                </span>
                            </div>
                        </article>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xl-8 col-lg-12">
                    <div class="card mb-4">
                        <article class="card-body">
                            <h5 class="card-title">Sale statistics Monthly</h5>
                            <canvas id="myChart" height="120px"></canvas>
                        </article>
                    </div>
                    
                </div>
                <div class="col-xl-4 col-lg-12">
                    <div class="card mb-4">
                        <article class="card-body">
                            <input type="hidden" id="orderStatus" value="<%= JSON.stringify(orderStatus) %>">
                            <h5 class="card-title">order Statuses</h5>
                            <canvas id="myChart2" height="217"></canvas>
                        </article>
                    </div>
                </div>

                <div class="col-xl-8 col-lg-12">
                    <div class="card mb-4">
                        <article class="card-body">
                            <h5 class="card-title">Sale statistics Daily</h5>
                            <canvas id="myChart3" height="120px"></canvas>
                        </article>
                    </div>
                    
                </div>
            </div>

            <div style="display: none;" id="chart-container">
                <canvas id="myChartData"   data-monthly="<%= chartData.monthlyData %>" data-daily="<%= chartData.dailyData %>">
                </canvas>
            </div>
            
         
        </section> <!-- content-main end// -->
        

        <script>

           
        </script>